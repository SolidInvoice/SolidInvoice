        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/captainhook-phar": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bin-dir": "bin",
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "cp .env.dist .env",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        }
    }
