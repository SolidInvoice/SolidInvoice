        "symfony/phpunit-bridge": "^5.0",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.3.5"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "symfony": {
            "allow-contrib": true
        }
    },
    "autoload": {
        "psr-4": {
            "SolidInvoice\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "cp .env.dist .env",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        }
    }
}
