<div {{ attributes }}>
    {{ form_start(form, {
        attr: {
            'data-action': 'live#action:prevent',
            'data-live-action-param': 'save'
        }
    }) }}
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">
                Add {{ notificationType }} Notification Integration
            </h3>
        </div>
        <div class="card-body" data-skip-morph>
            {{ form_row(form) }}
        </div>
        <div class="card-footer">
            <button type="submit" class="btn btn-primary">{{ icon('save') }} {{ 'Save'|trans }}</button>
        </div>
    </div>
    {{ form_end(form) }}
</div>
